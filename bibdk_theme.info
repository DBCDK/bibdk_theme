;==================================
;         FORMALLY
;==================================
name = Bibliotek.dk
description = Custom theme for bibliotek.dk
screenshot = screenshot.png
core = 7.x
version = 7.x-0.80


;==================================
;          CSS
;==================================
stylesheets[all][] = build/css/screen.css

;==================================
;        JS BIBDK
;==================================
scripts[] = build/js/header.js


;==================================
;        JS LIBS
;==================================
scripts[] = build/js/libs.js

; migrate creates objects deprecated from jquery 1.1.9 e.g. browser object
scripts[] = libs/jquery-migrate/jquery-migrate-1.2.1.min.js

;==================================
;      JS FOUNDATION
;==================================
scripts[] = build/js/foundation-concatenated.js


;==================================
;        REGIONS
;==================================
regions[search_panel] = Search panel
regions[content] = Content
regions[carousel] = Carousel
regions[subjects] = Subjects
regions[sidebar] = Sidebar
regions[banner] = Banner
regions[footer] = Footer


;==================================
;         PANELS
;==================================
plugins[panels][layouts] = templates/layouts
